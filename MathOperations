using System;

namespace MathOperations
{
    public static class MathLib
    {
        // Method to multiply two numbers and print the result
        public static void Multiply(double num1, double num2)
        {
            double result = num1 * num2;
            Console.WriteLine($"{num1} * {num2} = {result}");
        }

        // Method to multiply three numbers and print the result
        public static void MultiplyThree(double num1, double num2, double num3)
        {
            double result = num1 * num2 * num3;
            Console.WriteLine($"{num1} * {num2} * {num3} = {result}");
        }
    }
}
